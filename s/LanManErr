
        GBLS    FSName
FSName  SETS    "LanMan"

        GBLS    fcerror
fcerror SETS    "FSErr"

fsnumber_LanMan         * 102

        MACRO
$num    FSErr   $name, $str
        =       "        AddError $FSName.$name, ""$str"", &"
        =       :STR: (&10000 + fsnumber_$FSName:SHL:8 + &$num), 10
        MEND

        GET     ListOpts
        GET     FSNumbers

        AREA    Text, DATA

        ; File header
        =       "; This is an autogenerated file, do not edit", 10, 10

        ; Define all the errors that FileCore generates for the FS
        GET     FileCoreErr

        ; First, internal LanManFS errors, starting at &F0

F0      FSErr   BadFileCore, LanManFS requires FileCore 3.31 or later
F1      FSErr   BadOSVer, LanManFS requires RISC OS 3.00 or later
F2      FSErr   BadCmdLine, Bad LanManFS command line
F3      FSErr   InitFailed, Initialisation failed
F4      FSErr   IntError, LanManFS internal error
F5      FSErr   OutOfMemory, Memory allocation failed

        ; Define additional SMB errors, starting at &40
        ; NTSTATUS errors not covered by existing FileCore errors

40      FSErr   BadLogonType, Logon type not granted to this account
41      FSErr   NoLogonServers, Server can't talk to domain controller
42      FSErr   ServerException, Server software exception
43      FSErr   AcctDisabled, Account disabled
44      FSErr   AcctRestricted, Account restricted
45      FSErr   BadLoginTime, Login time restriction
46      FSErr   AcctLockedOut, Account locked out
47      FSErr   BadAcctName, Invalid account name
48      FSErr   PwdTooShort, Password too short
49      FSErr   PwdTooRecent, Password too recent
4A      FSErr   PwdHistConflict, Password history conflict
4B      FSErr   AcctExpired, Account expired
4C      FSErr   PwdExpired, Password expired
4D      FSErr   PwdRestricted, Password restriction
4E      FSErr   PwdMustChange, Password must be changed
4F      FSErr   BadLogonHours, Logon hours restricted
50      FSErr   AddrExists, Transport address already exists
51      FSErr   BadNetName, Invalid network name
52      FSErr   NetNameDeleted, Network name deleted
53      FSErr   DupName, Duplicate name
54      FSErr   BadNetPath, Invalid network path
55      FSErr   BufTooSmall, Buffer too small
56      FSErr   BadBufSize, Bad buffer size
57      FSErr   AddrConflict, Conflicting addresses
58      FSErr   ConnAborted, Connection aborted
59      FSErr   Disconnected, Connection disconnected
5A      FSErr   ConnRefused, Connection refused
5B      FSErr   ConnReset, Connection reset
5C      FSErr   HostUnreachable, Host unreachable
5D      FSErr   NetUnreachable, Network busy or unreachable
5E      FSErr   PortRefused, Port connection refused
5F      FSErr   PortUnreachable, Port unreachable
60      FSErr   ProtocolError, Network protocol error
61      FSErr   EndOfFile, End of file
62      FSErr   FileIsDir, File is a directory
63      FSErr   IsConnected, Socket is already connected
64      FSErr   BadPwdFormat, Ill-formed password
65      FSErr   BadHandle, Invalid handle
66      FSErr   BadParam, Invalid parameter
67      FSErr   PipeError, Named pipe operation failed
68      FSErr   BadWorkstation, Invalid workstation
69      FSErr   IPAddrConflict, IP address conflict
6A      FSErr   LicenceError, Licence quota exceeded
